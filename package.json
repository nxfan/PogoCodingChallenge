{
  "name": "PogoCodingChallenge",
  "version": "1.0.0",
  "description": "Pogo Coding Challenge",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "webpack --mode development && babel src -d dist",
    "start": "npm run build && node dist/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --watch src -x 'npm run build &&  env-cmd -f src/config/dev.env node dist/server/index.js'",
    "dev": "npm run build && concurrently \"npm run server\" \"npm run client\""
  },
